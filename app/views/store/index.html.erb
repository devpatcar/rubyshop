<div class="row">
<div class="col-xs-12 text-right">
<h1 class="cart-text"><%= render 'cart_text' %></h1>
</div>
</div>


  <% @products.each do |product| %>
    <%= render "product_row", product: product, order_item: @order_item %>
  <% end %>

</div>
<% if @allowPagination %>
<nav aria-label="Page navigation" class="row">
<ul class="pagination">
  
  <% if @isNotFirstPage %>
  <li>
  <%= link_to(content_tag(:span, "<<") , store_path(:page => @firstPage)) %>
  </li>
  <% end %>
    
  
  <% for i in 1..@pageCount do %>
    <li><%= link_to i, store_path(:page => i) %></li>
  <% end %> 

  <% if @isNotLastPage %>
  <li>
  <%= link_to(content_tag(:span, ">>") , store_path(:page => @lastPage)) %>
  </li>
  <% end %>
    
 
</ul>
</nav>
<% end %>


